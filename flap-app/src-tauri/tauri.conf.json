{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "flap-app",
  "version": "0.1.0",
  "identifier": "org.bird.flap",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Flap",
        "width": 800,
        "height": 500
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/flap-icon.png"
    ]
  }
}
