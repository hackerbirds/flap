{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Flap",
  "version": "0.4.0",
  "identifier": "org.bird.flap",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Flap",
        "width": 800,
        "height": 500
      }
    ],
    "security": {
      "csp": null,
      "capabilities": []
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.jpg",
      "icons/icon.ico"
    ]
  }
}
